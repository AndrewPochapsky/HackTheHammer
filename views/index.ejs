        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Dashboard</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-3 col-md-6">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <div class="row">
                                <div class="col-xs-3">
                                    <h1>Protein</h1>
                                </div>
                                <div class="col-xs-9 text-right">
                                    
                                    <div class="huge">26</div>
                                    <div>Total</div>
                                </div>
                            </div>
                        </div>
                        
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="panel panel-green">
                        <div class="panel-heading">
                            <div class="row">
                                <div class="col-xs-3">
                                    <h1>Sugars</h1>
                                </div>
                                <div class="col-xs-9 text-right">
                                    <div class="huge">12</div>
                                    <div>Total</div>
                                </div>
                            </div>
                        </div>
                        
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="panel panel-yellow">
                        <div class="panel-heading">
                            <div class="row">
                                <div class="col-xs-3">
                                    <h1>Sodium</h1>
                                </div>
                                <div class="col-xs-9 text-right">
                                    <div class="huge">124</div>
                                    <div>Total</div>
                                </div>
                            </div>
                        </div>
                        
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="panel panel-red">
                        <div class="panel-heading">
                            <div class="row">
                                <div class="col-xs-3">
                                   <h1>Fats</h1>
                                </div>
                                <div class="col-xs-9 text-right">
                                    <div class="huge">13</div>
                                    <div>Total</div>
                                </div>
                            </div>
                        </div>
                
                    </div>
                </div>
            </div>
            <!-- /.panel -->
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-clock-o fa-fw"></i> Responsive Timeline
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <ul class="timeline">
                                
                              <%  function round(value, precision) { %>
                                 <%   var multiplier = Math.pow(10, precision || 0); %>
                                 <%   return Math.round(value * multiplier) / multiplier; %>
                               <%  } %>
                                <% for(x= body.length-1; x>=0; x--){ %>
                                    
                              <% var t = "timeline-inverted" %>
                              <%  if(x%2 !== 0){ %>
                                <li class="<%= t %>">
                               <% }else{ %>
                              
                                 <li>
                                 <%   } %>
                                    <div class="timeline-badge"><i class="fa fa-check"></i>
                                    </div>
                                    <div class="timeline-panel">
                                        <div class="timeline-heading">
                                            <h4 class="timeline-title"><%= body[x].name %></h4>
                                            <p><small class="text-muted"><i class="fa fa-clock-o"></i> <%= body[x].time %></small>
                                            </p>
                                        </div>
                                        <div class="timeline-body">
                                            <ul>
                                                
                                                <li> Calories: <%= round(body[x].calories, 1) %> kCal</li>
                                                <li> Protein: <%= round(body[x].protein, 1) %> grams</li>
                                                <li> Sugar: <%= round(body[x].sugar, 1) %> grams</li>
                                                <li> Sodium: <%= round(body[x].sodium, 1) %> mg</li>
                                                <li> Fat: <%= round(body[x].fat, 1) %> grams</li>
                                                
                                            </ul>
                                        </div>
                                    </div>
                                </li>
                                
                               <% }%>
                                
                            </ul>
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                
                        <!-- /.panel-footer -->
                    </div>
                    <!-- /.panel .chat-panel -->
                </div>
                <!-- /.col-lg-4 -->
            </div>
